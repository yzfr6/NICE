package algorithm.objects;

import java.util.List;

public class Node {

	public int db_id;
	public String code;
	public String text;
	public String type;
	
	public Entity single;
	public List<Entity> and;
	public List<Entity> or;
	
	
	public Node(int db_id, String code, String text, String type, Entity single, List<Entity> and, List<Entity> or) {
		super();
		this.db_id = db_id;
		this.code = code;
		this.text = text;
		this.type = type;
		this.single = single;
		this.and = and;
		this.or = or;
	}
	
	
	
}
