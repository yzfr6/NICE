package responses;

public class AuthorisationResponse {
	public boolean success;
	public String message;
	public int code;
	public int user_id;

	public AuthorisationResponse(boolean success, String message, int code) {
		super();
		this.success = success;
		this.message = message;
		this.code = code;
		user_id = -1;
	}

	public AuthorisationResponse(boolean success, String message, int code, int user_id) {
		super();
		this.success = success;
		this.message = message;
		this.code = code;
		this.user_id = user_id;
	}

}